<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/pic_seekbar_corner_radious" />
            <gradient android:startColor="@color/pic_seekbar_bg_startcolor" android:endColor="@color/pic_seekbar_bg_startcolor" android:angle="270.0" android:centerY="0.5" android:centerColor="@color/pic_seekbar_bg_centercolor" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="30.0dip" />
                <gradient android:startColor="@color/pic_seekbar_startcolor" android:endColor="@color/pic_seekbar_startcolor" android:angle="270.0" android:centerColor="@color/pic_seekbar_centercolor" />
            </shape>
        </clip>
    </item>
</layer-list>